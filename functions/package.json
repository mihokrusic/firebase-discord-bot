{
    "name": "functions",
    "scripts": {
        "lint": "tslint --project tsconfig.json",
        "build": "tsc",
        "watch": "tsc -w",
        "serve": "firebase functions:config:get > .runtimeconfig.json && npm run build && firebase emulators:start --only functions",
        "shell": "firebase functions:config:get > .runtimeconfig.json && npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "8"
    },
    "main": "lib/index.js",
    "dependencies": {
        "@google-cloud/pubsub": "^1.7.3",
        "axios": "^0.19.2",
        "firebase-admin": "^8.10.0",
        "firebase-functions": "^3.6.1",
        "mongoose": "^5.9.12",
        "prettier": "^2.0.5"
    },
    "devDependencies": {
        "tslint": "^5.12.0",
        "typescript": "^3.8.0",
        "firebase-functions-test": "^0.2.0"
    },
    "private": true
}
